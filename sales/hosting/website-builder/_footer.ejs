<script type="text/javascript">

  var ytPlayerList = {}; // Maps youtube player with its appropiate iframe based on the iframe id

  function onYouTubeIframeAPIReady() {
      $('.embedVideo').gdVideos();
  }

  function onPlayerReady (event) {
      ytPlayerList[$(event.target.getIframe()).attr('id')] = event.target;
  }

  $(function () {
    if (!$('html').hasClass('lt-ie8')) {
      if ($('.embedVideo').length > 0) {
        var tag = document.createElement('script'),
        firstScriptTag = document.getElementsByTagName('script')[0];
        tag.src = "//www.youtube.com/iframe_api";
        firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);
      }
    } else {
      $('.embedVideoIE7').gdVideos();
    }
    $('.rating-stars-init').ratingStars();
    $('.isToolTip').tooltip();
    $('.equal-height').setColumnsEqualHeight();
    $('.select-box').setSelectBox();
    $('.tab-link').tabLinks();

  });


  $('.js-open-overview').on('click', function(e){
    e.preventDefault();
    window.location.hash = '';
    $('a[href="#plans-pricing"]').tab('show');
    window.location.hash = '#plans-pricing';
  });

</script>