    function IEVersion(){
      var ua = window.navigator.userAgent;
      if (ua.indexOf('MSIE 8')>0 || ua.indexOf('MSIE 9')>0){
        return '8-9';
      }
      else if(ua.indexOf('10-current')>0 || ua.indexOf('Trident')>0){
        return '10-current';
      }
    }
    var ieVersion = IEVersion();
      if(ieVersion != undefined)
        if(ieVersion == '10-current'){
