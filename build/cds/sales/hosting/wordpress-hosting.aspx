            var htmlTokenized = $modal.html();
            htmlTokenized = htmlTokenized.replace(/\{0\}/gi, price0);
            htmlTokenized = htmlTokenized.replace(/\{1\}/gi, price1);
            $modal.html(htmlTokenized);
          });
        
