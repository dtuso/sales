        var maxIconHeight = 0;
        $(".plan-image").each(function(index, image) {
          maxIconHeight = $(image).height() > maxIconHeight ? $(image).height() : maxIconHeight;
        }).css({height: maxIconHeight, marginBottom: 10});
      
        // auto height the plan boxes
