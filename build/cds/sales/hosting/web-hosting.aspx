            <script type="text/javascript">
              var itemTrackingCode = "slp_hosting_4GH";
              
              $(".btn-plan").click(function() {
              
                if ("false" != "false"){
                  var productTile = $(this).data("ul");
              
                  var productPackage = $("ul#"+productTile+" li").filter('[data-selected="true"]').attr('data-plan');
