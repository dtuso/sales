    var sapiurl = '[@T[link:<external linktype="SALESPRODUCTSURL" path="/v1/pl/1/cart/packages/'+plan+'" />]@T]';
    var pkg = {};
    pkg.pkgid = plan;
    pkg.itc = offerInfo.itcCode;
    pkg.quantity = 1;
    pkg.ci = ciCode;
    pkg.custom = { "domain": domainToAdd }
    var postdata = "requestData=" + JSON.stringify(pkg);
      $.ajax({
         type:"POST",
