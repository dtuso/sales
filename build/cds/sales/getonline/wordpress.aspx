            var maxHeight = 0;
            $(outerPlan).find(".pro-plan-wrap").each(function(index, plan) {
              maxHeight = $(plan).outerHeight() > maxHeight ? $(plan).outerHeight() : maxHeight;
            });
            if( maxHeight > 0 )$(outerPlan).find(".pro-plan-wrap").css("height", maxHeight);
          });
        }
      });
      
    </script>
