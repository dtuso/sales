                        $videoInfo.css({'display':'none'});
                    });
                    //$(video).css({'opacity':0.0}).animate({'opacity':1.0});
                },
                {'autoplay':1,'controls':2,'showinfo':0,'playsinline':1,'modestbranding':1,'rel':0,'iv_load_policy':3},
                $(".video-marquee"),
                'prepend'
            );
          }).css({'cursor':'pointer'});
        });
      });
      
