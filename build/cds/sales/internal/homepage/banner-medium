      var tms = function(data) {
        $(document).ready(function(){
          for (var key in data) {
            if ((data[key]) && (data[key].trim() != '')) {
              var elem = document.getElementById(key)
              if (elem != null) {
                $('#' + key).html(data[key]);
              }
            }
          }
        });
      }
      
