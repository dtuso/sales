</atlantis:webstash>
div.aux-content-wrapper
  div.aux-content
    span.tld-links
      each tld in bugBar
        if tld.co
          a(href!="[@T[link:<relative path='/tlds/co-domain.aspx'><param name='ci' value='93296' /><param name='tld' value='CO' /></relative>]@T]" data-ci="93296")
            span#co-icon.tld-icon
            span#co-price
              [@T[productprice:<current productid="56401" dropdecimal="false" period="yearly" htmlsymbol="false" negative="parentheses" />]@T]
        if tld.org
          a(href!="[@T[link:<relative path='/tlds/org.aspx'><param name='ci' value='93297' /><param name='tld' value='ORG' /></relative>]@T]" data-ci="93297")
            span#org-icon.tld-icon
            span#org-price
              [@T[productprice:<current productid="12101" dropdecimal="false" period="yearly" htmlsymbol="false" negative="parentheses" />]@T]*
        if tld.net
          a(href!="[@T[link:<relative path='/tlds/net.aspx'><param name='ci' value='93298' /><param name='tld' value='NET' /></relative>]@T]" data-ci="93298")
            span#net-icon.tld-icon
            span#net-price
              [@T[productprice:<current productid="12001" dropdecimal="false" period="yearly" htmlsymbol="false" negative="parentheses" />]@T]*
    div.gdhp-share
      div.fosshare(data-twshareurl!="[@T[link:<external linktype='XDOTCO' path='/wwwGD' />]@T]" data-twtext!="[@L[cds.sales/homepage:twitter-text-gdbl]@L]" data-twrelated="GoDaddy" data-shareurl!="[@T[link:<relative path='/'/>]@T]" data-buttons="google,facebook,twitter")