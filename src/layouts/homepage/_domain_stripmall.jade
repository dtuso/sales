<atlantis:webstash type="css">
style.
  .aux-content-wrapper{height:45px;background-color:#fff;width:100%}
  .aux-content{margin:0 auto;position:relative;width:1000px;padding-top:13px}
  .gdhp-share{position:relative;float:right;}
  .tld-links>a{font-size:14px;text-decoration:none;color:#333}
  .tld-links>a>span{position:relative;}
  .tld-links>a:hover{text-decoration:underline}
  .tld-icon{display:inline-block;}
  #co-icon{position:absolute;height:20px;width:19px;margin:-2px 8px 0 8px;background:url([@T[link:<imageroot />]@T]fos/hp/rebrand/img/gdhp_sprite_20140923.png) no-repeat -891px -9px;}
  #co-price{padding-left:35px;}
  #org-icon{position:absolute;width:38px;height:16px;margin:4px 0 0 5px;background:url([@T[link:<imageroot />]@T]fos/hp/rebrand/img/gdhp_sprite_20140923.png) scroll -844px -8px no-repeat;}
  #org-price{padding-left:50px;}
  #net-icon{background:url([@T[link:<imageroot />]@T]fos/hp/rebrand/img/gdhp_sprite_20140923.png) scroll -799px -9px no-repeat;height:16px;margin:2px 0 0 8px;position:absolute;width:42px;}
  #net-price{padding-left:50px;}
