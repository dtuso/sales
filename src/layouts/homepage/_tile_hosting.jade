<atlantis:webstash type="css">
style.
  .hosting-tile{background-color:#fff;color:#58585a}
  .hosting-tile-logo{background:url([@T[link:<imageroot />]@T]fos/hp/rebrand/img/gdhp_sprite_20140917.png) no-repeat -416px -513px;width:440px;height:140px;margin:30px 20px 20px}
  .hosting-tile-text{margin:0 10px 0 20px}
  .hosting-tile-headline{font-size:32px;line-height:32px}
  .hosting-tile-maintext{font-size:35px;line-height:30px;font-family:'Walsheim-Black'}
