  .hosting-tile>.atn-btn-wrap{margin:20px}
  .hosting-tile>.atn-btn-wrap>.btn-lrg{width:220px;float:left;margin-right:10px}
  .hosting-tile>.atn-btn-wrap>span>span.hosting-tile-pricetext{font-family:'Walsheim-Light';font-size:30px;line-height:30px;margin-bottom:5px}
  .hosting-tile>.atn-btn-wrap>span>span.hosting-tile-price{font-family:'Walsheim-Bold';font-size:29px;line-height:29px}
