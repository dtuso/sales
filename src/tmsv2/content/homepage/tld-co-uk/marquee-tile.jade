extends ../../../../sales/homepage/_marquee-tile-tld.jade

block prepend vars
  - dataMessageName = "tld-co-uk";
  - dataCi = "96168";
  - headline = '[@T[productcompare:<percent primaryproductid="57465" secondaryproductid="57465" showsymbol="true"/>]@T] OFF 100% BRILLIANT';
  - headlineNoDiscount = '100% BRILLIANT';
  - tldPfid = "57465";
  - tld = "co-uk";
  - tldLeafUrl = "/tlds/co-uk.aspx";
  - tldSVG = "data:image/svg+xml;base64,";
  - tldFallbackPNG = "data:image/png;base64,";
  - imageWidth = "25%"

block append tileStyles
  style.
    .co-uk.marquee-tile-tld{background-color:#dedede}
    .co-uk .price-text{color:#333}

block tileContent
  img.tld.svgfallback(src!=tldSVG)
  <!--[if lt IE 9]>
  img.img-responsive(src!=tldFallbackPNG)
  <![endif]-->
  | ##if(productHasDiscountMoreThan(57465, 5)) 
  | 
  .headline !{headline}
  | 
  | ##else
  | 
  .headline !{headlineNoDiscount}
  | 
  | ##endif
  .cta 
    span.price-text STARTING AT 
    span.price [@T[productprice:<current productid="57465" period="yearly" />]@T]/YR
