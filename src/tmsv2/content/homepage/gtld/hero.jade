atlantis:webstash(type="css")
  style
    :less
      #marquee{background-color:#f47f00;font-size:12px;}
      #gtld-hero{color:#fff;
        .headline {font-size:1.3em;padding-top:3.5em;line-height:1em}
        .maintext{font-size:3.15em;padding-top:.1em;line-height:.8em;}
        .subtext{font-size:1em;margin:1em 0}
        .gtld-phone{margin-top:31px;background-color:#fff;border:solid #000;border-width:10px 7px 0 7px;
          ul{list-style:none;margin-bottom:0;padding:0;}
          li{margin:0;
            a{text-decoration:none;color:#333;display:block;height:40px;padding:10px 0;}
            .gtld {font-size:1.2em;display:inline-block;float:left;margin-left:5px;}
            .price-container {font-size:1em;display:table;float:right;text-align:right;}
            .price {display:table-cell;vertical-align:middle;color:#f2812e;padding-right:5px}  
            .price strike, .price nostrike {display:block;color:#333;}
          }
          li:last-child{border-bottom:0px;}
          li:hover a {color:#f5811f;}
        }
      }
      @media only screen and (min-width:480px) {
        #gtld-hero{
          .gtld-phone{border-width:0;background-color:transparent;background:url(data:image/png;base64,) no-repeat center center;background-size:cover;
            ul{padding:40px 50px 0 50px;}
            li{
              .price-container{font-size:1.2em;}
            }
          }
        }
      }
      @media only screen and (min-width:768px) {
        #gtld-hero{
          .gtld-phone{
            ul{padding:30px 50px 0 50px;}
            li{border-bottom:#b9babb solid 1px;
              .price{padding-right:15px;}
            }
          }
        }
      }
      @media only screen and (min-width:992px) {
        #marquee{font-size:14px}
        #gtld-hero{
          .headline {padding-top:1.5em;line-height:1em}
          .maintext{padding-top:.1em}
          .subtext{margin:1em 0}
          .gtld-phone{margin-top:31px;
            ul{padding:30px 50px 0 50px;}
            li{
              a{height:40px;}
              .price{padding-right:15px}
            }
          }                 
        } 
      }
      @media only screen and (min-width:1200px) {
        #marquee{font-size:16px}
        #gtld-hero{ 
          .headline {padding-top:3.5em;line-height:1.3em}
          .maintext{padding-top:0}
          .subtext{margin:2em 0;}
          .gtld-phone{margin-top:0;
             ul{padding:55px 50px 0 49px;}
             li{
              a{height:49px;}
              .price{padding-right:0}
              .arrow{background:url([@T[link:<imageroot />]@T]fos/hp/rebrand/img/gdhp_sprite_20141024.png) scroll -392px -10px no-repeat;width:8px;height:35px;display:inline-block;float:right;margin:0 10px;}
             }
          } 
        }
      }

