extends ../../../../sales/homepage/_tile-2col_base.jade

append vars
  - ctaLink = "[@T[link:<relative path='domains/domain-name-search.aspx'><param name='ci' value='11111' /></relative>]@T]";
  - dataCi = "11111";
  - containerClass = "com-sale-in-tile";

block append tileStyles
  style
    :less
      .com-sale-in-tile{color:#fff;background-size: cover;background-position: center center;background-repeat: no-repeat;background-color:#333;
        h2{font-size:2em;margin:10px 0 5px}
        h3{font-size:2.1em;margin:0;color:#78C044}
        .cta{font-size:2.7em;color:#ff8a00;line-height:.8em}
        .disclaimer{font-size:1em;margin:15px 0 10px;}
      }
      @media only screen and (min-width:768px) {
        .com-sale-in-tile{background-image:url(data:image/jpeg;base64,);background-position-x:-28px;
          h2{font-size:1.6em;margin:10px 0 5px}
          h3{font-size:1.8em;margin:0;color:#78C044}
          .cta{font-size:2.4em;color:#ff8a00;line-height:.8em}
          .disclaimer{font-size:.7em;}
        }
      }      
      @media only screen and (min-width:992px) {
        .com-sale-in-tile{background-position-x:-65px;
          h2{font-size:1.0em;margin:10px 0 5px}
          h3{font-size:1.3em;margin:0;color:#78C044}
          .cta{font-size:2em;color:#ff8a00;line-height:.8em}
          .disclaimer{font-size:.5em;margin:15px 2px 10px;}
        }
      }
      @media only screen and (min-width:1200px) {
        .com-sale-in-tile{background-position-x:-27px;
          h2{font-size:1.2em;margin:10px 0 5px}
          h3{font-size:1.5em;margin:0;color:#78C044}
          .cta{font-size:2em;color:#ff8a00;line-height:.8em}
          .disclaimer{font-size:.5em;margin:15px 0 10px;}
        }
      }

block leftContent
  .header-wrapper
    h2.header-text.walsheim-black Big dreams. Small price.
    h3.maintext.walsheim-black NEW .COM
    .cta.walsheim-black(data-tokenize!="[@T[productprice:<current productid='101' period='yearly' promocode='444888' />]@T]") {0}*
    .disclaimer(data-tokenize!="[@T[productprice:<current productid='10101' period='yearly' checktmspromocode='true' />]@T]") *2 year minimum registration required. Subsequent years charged at {0}/yr.
block rightContent