extends ../_hero-left-arrow.jade

append vars
  - var classes = "tld-com-flash-jazzy"
  - var ciCode = 99212;
  - var ctaLink = "[@T[link:<relative path='/domains/search-new.aspx'><param name='ci' value='"+ciCode+"' /></relative>]@T]";
  - var polyColor = "white";

append styles
  style
    :less
      #hero-wrapper .tld-com-flash-jazzy {
        .right-container{background:#77c043; .svgfallback, .img-responsive{width:225px;height:220px;}}
        .left-container{
          .left-content {
          .svgfallback{width:4.5em;height:1em;}
          .disclaimer{padding-top:4em;}
          .price{padding:.75em 0 .75em 0;}
          .disclaimer{text-transform: initial;}
          .text-purchase{
            font-size:2.25em;
            sup {
              color: black;
              font-size: 0.65em;
            }
          }
          .bold{font-size: 1.3em;}
          .black{margin: 0.35em 0 0.65em;}
          }
          
        }
      }

block leftContent
  mark.bold [@L[cds.tmsv2/content/homepage/tld-com-flash/hero:flag-date]@L]
  h2.text-purchase [@L[cds.tmsv2/content/homepage/tld-com-flash/hero:headline]@L]
  p [@L[cds.tmsv2/content/homepage/tld-com-flash/hero:sub-headline]@L]
  a.black.btn.btn-md.btn-primary [@L[cds.tmsv2/content/homepage/tld-com-flash/hero:cta]@L]
  <!--[if lt IE 9]>
  img.img-responsive(src!="[@T[link:<imageroot />]@T]fos/tmsv2/content/homepage/tld-com-flash-jazzy/com-logo_2x.png")
  <![endif]-->
  img.svgfallback(src="data:image/svg+xml;base64,")
  p.font-sm.disclaimer [@L[cds.tmsv2/content/homepage/tld-com-flash/hero:disclaimer]@L]
block rightContent
  <!--[if lt IE 9]>
  img.img-responsive(src!="[@T[link:<imageroot />]@T]fos/tmsv2/content/homepage/tld-com-flash-jazzy/stopwatch.png")
  <![endif]-->
  img.svgfallback(src="data:image/svg+xml;base64,")
