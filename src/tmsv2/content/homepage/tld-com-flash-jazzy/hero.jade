extends ../_hero-left-arrow.jade

append vars
  - var classes = "tld-com-flash-jazzy"
  - var ciCode = 99212;
  - var ctaLink = "[@T[link:<relative path='/tlds/com.aspx'><param name='ci' value='"+ciCode+"' /></relative>]@T]";
  - var polyColor = "white";

append styles
  style
    :less
      #hero-wrapper .tld-com-flash-jazzy {
        .right-container{background:#77c043;}
        .left-container{
          .left-content {}
          .disclaimer{padding-top:4em;}
          .price{padding:.75em 0 .75em 0;}
          .disclaimer{text-transform: initial;}
          .text-purchase{
            font-size:2.25em;
            sup {
              color: black;
              font-size: 0.65em;
            }
          }
          .bold{font-size: 1.3em}
          .black{margin: 0.35em 0 0.65em;}
          .svgfallback{width:20%;}
        }
      }

block leftContent
  mark.bold() Memorial Day Weekend Only!
  h2.text-purchase() [@T[productprice:<current productid="101" period="yearly" checktmspromocode="true" htmlsymbol="false" />]@T] .com*
    sup() /1st yr
  p Save on the first year when you register for two.
  a.black.btn.btn-md.btn-primary() Find your .com
  <!--[if lt IE 9]>
  img.img-responsive(src!="[@T[link:<imageroot />]@T]fos/tmsv2/content/homepage/tld-com-flash-jazzy/com-logo_2x.png")
  <![endif]-->
  img.svgfallback(src="data:image/svg+xml;base64,")
  p.font-sm.disclaimer *2 year minimum registration required. Subsequent years charged at [@T[productprice:<list productid='101' dropdecimal='false' period='yearly' negative='parentheses' />]@T]/yr
block rightContent
  <!--[if lt IE 9]>
  img.img-responsive(src!="[@T[link:<imageroot />]@T]fos/tmsv2/content/homepage/hosting-flash-jazzy/sale-banner.png")
  <![endif]-->
  img.svgfallback(src="data:image/svg+xml;base64,")
