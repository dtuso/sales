atlantis:webstash(type="css")
  style
    :less
      #marquee { 
        font-size:16px;
        background-color: #f3f3f4; overflow-y: hidden;
        .promo-wrapper, .price-wrapper { margin-top: 20px; }
        .inline-dash { display:inline-block;overflow:hidden;letter-spacing:-0.5em;font-size:1.4em;vertical-align:top;line-height:50%; }
        .headline-wrapper { margin-top: 1em; line-height: 1; }
        .promo-wrapper { color: #77c043; }
        .disclaimer { font-size: 0.6em; margin-bottom:2%}
        .hero-wrapper > .row img { margin: 0 auto; display: block; }
        .dash{letter-spacing:-.3em}
        button.btn-purchase{margin:3% 0}
        .line1,.line2{color:#008B2D}
        .chips{max-height:219px;height:100%}
        .line1,.line2{font-size:1.2em;}
        .line3{font-size:1.67em}
        .line5{font-size:2.7em}
        .disclaimer{}
      }
      @media only screen and (min-width: 401px){
        #marquee{font-size:29px}
      }
      @media only screen and (min-width: 768px){
        #marquee{
          font-size:16px;
          .headline-wrapper{margin-top:2.4em}
          .chips{max-height:none}
        }
      }
      @media only screen and (min-width: 992px){
        #marquee{
          font-size:20px;
          .image{
          padding-left:4%;
          padding-right:4%;
          }
        }
      }
      @media only screen and (min-width:1200px){
        #marquee{
          font-size:24px;
          .image{
            padding-left:6%;
            padding-right:6%;
          }
        }
      }
