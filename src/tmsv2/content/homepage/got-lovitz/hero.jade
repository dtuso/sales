extends ../../../../sales/homepage/_hero-polygon.jade
prepend vars
  - var classes = "got lovitz"
  - var dataCI = 96896;
  - var ctaLink = "[@T[link:<relative path='/offers/online-business.aspx'><param name='ci' value='"+dataCI+"'/></relative>]@T]";
  - var marqueeBackground = "[@T[link:<imageroot />]@T]fos/hp/rebrand/img/got-lovitz.jpg";
  - var polygon = "grey-mid";
  - var xsBannerColor = "#333";

append heroStyles
  style
    :less
      #marquee{background-position-x:43%;
        .hero.polygon.lovitz.got { color:#fff; margin-left:-10%;
          .headline { line-height:.9; padding-bottom:2%;
            .line1 {font-size: 3em;}
            .line2 {font-size: 4em;}
          }
          div.products{ padding-bottom:2%;
            >div{display:inline; font-size: 1em;}
          }
          .pricetext {color:#ff8a00; font-size: 1.1em; padding-bottom:2%}
          button {margin-bottom:2%}
          .disclaimer {font-size: .6em; padding-bottom:3%}
          img.products {width:270px; margin-left:19%}
          .website {margin-left:40px}
          .email {margin-left:55px}
        }
      }

block heroContent
  .headline
    .line1.walsheim-bold SILENCE
    .line2.tungsten THE DOUBTERS.
  img.products.svgfallback(src="data:image/svg+xml;base64,")
  <!--[if lt IE 9]>
  img.products(src!='data:image/png;base64,')
  <![endif]-->
  .products.walsheim-bold
    .domain DOMAIN
    .website WEBSITE
    .email EMAIL
  .cta
    .pricetext.walsheim-black ALL STARTING AT JUST [@T[multipleproductprice:<current productidlist="464069|101|7524" period="monthly" promocode="24681357" />]@T]/MO
    button.btn.btn-md.btn-purchase GET STARTED
  .disclaimer First year only. Annual purchase required.