extends ../../../../sales/homepage/_right_arrow.jade

block append heroVars
  - rightArrow.ci = "95854";
  - rightArrow.ctaLink = "[@T[link:<relative path='/pro'><param name='ci' value='"+rightArrow.ci+"'/></relative>]@T]";
  - rightArrow.classes += " gdpro-beginner-price";
  - rightArrow.arrowColor = "rgb(255,138,0)";
  - rightArrow.pngArrowFallback = "url([@T[link:<imageroot />]@T]fos/hp/rebrand/img/arrow-orange.png) no-repeat";
block append rightArrowStyles
  style
    :less
      .hero.right-arrow.gdpro-beginner-price{
        .rightContent {
        .line1{padding-bottom:2%}
        }
      }
block leftContent
  <!--[if lt IE 9]>
  img.img-responsive.fallback(src!='[@T[link:<imageroot />]@T]fos/hp/rebrand/img/gd-pro-logo-reversed.png')
  <![endif]-->
  img.img-responsive.svgfallback(src!='data:image/svg+xml;base64,')
  .bigtext
    .line1.tungsten
      span BUILT FOR PROS 
      span.dash ——— 
    .line2.walsheim-black PRICED FOR BEGINNERS.
    | 
    | ##if(productHasSavingsMoreThan(32759, 32759, 0))
    .line3.tungsten SAVE UP TO [@T[productcompare:<percent primaryproductid="32759" secondaryproductid="32759" showsymbol="true" />]@T].
    | 
    | ##endif
block rightContent
  .bigtext
    .line1.walsheim-black VISIT GODADDY PRO
  button.btn.btn-lg.btn-purchase CHECK IT OUT