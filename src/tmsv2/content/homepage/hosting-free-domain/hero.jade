extends ../../../../sales/homepage/_right_arrow.jade

block append heroVars
  - rightArrow.ci = "97796";
  - rightArrow.ctaLink = "[@T[link:<relative path='/hosting/web-hosting.aspx'><param name='ci' value='"+rightArrow.ci+"'/></relative>]@T]";
  - rightArrow.classes += " hosting-free-domain walsheim-black";
block append rightArrowStyles
  style
    :less
      .hero.right-arrow.hosting-free-domain{ font-size: 10px;
          .leftContent{font-size:1.9em;
            img {margin-bottom: 5%; width:100%; max-width:200px}
            .line1 {line-height:1}
            .line2 {font-size:.7em; color:#333; margin-top:5%}
          }
          .rightContent{ width: 90%;
            .line1{font-size:1.6em;}
            .price{font-size:1.3em;
              .space {word-spacing:-7px}
            }
          }
        @media only screen and (min-width: 768px){font-size:13px}
        @media only screen and (min-width: 1200px){font-size:16px}
      }
block leftContent

  img.svgfallback(src='data:image/png;base64,')
  <!--[if lt IE 9]>
  img(src!='data:image/png;base64,')
  <![endif]-->
  .line1 
  | [@L[cds.sales/homepage/marquee/web-hosting:product]@L]
  | ##if(productHasSavingsMoreThan(32051, 32051, 4))
  .line2 [@L[cds.sales/homepage/marquee/web-hosting:actAndSave]@L]
  | 
  | ##endif
block rightContent
  .line1 [@L[cds.sales/homepage/marquee/web-hosting:onlyProduct]@L]
  a.btn.btn-md.btn-purchase [@L[cds.sales/homepage/marquee/web-hosting:cta-text]@L]