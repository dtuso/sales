---
location: sales/homepage/tile/got/default_template
id: false
name: GCEMO-593
---
atlantis:webstash(type="css")
  style
    :less
      .got-tile{background-position:0 -485px; position:relative; font-family:'Walsheim-Black', 'Arial Black', sans-serif;overflow:hidden;
        .content-wrapper{padding:23px;}
        #addition-wrapper{font-size:56px; width:33%;margin-top:115px; display:inline-block;
          .addition{height:96px; margin-bottom:20px; line-height:0; padding:48px 0; height:0}
        }
        #products {width:66%; float:right;display:inline-block;font-size:24px;
          .product{height:96px;margin-bottom:20px; position:relative;
            .text{position: absolute;top: 50%;-webkit-transform: translateY(-50%);-ms-transform: translateY(-50%);transform: translateY(-50%); line-height:.9}
          }
        }
        .price-text {position:absolute; bottom:23px; left:23px;width:45%; color: #ff8a00;line-height:1.1;text-align:center;
            .price{font-size:40px;line-height:.9}
            .text{font-size:30px}
          }
        .cta {position:absolute;bottom:23px;right:23px;width:45%;
          a{max-width:80%; white-space:normal;}
          .disclaimer {color:#333; font-size:10px; font-family:  Arial, "Helvetica Neue", Helvetica, sans-serif;}
        }
      }
      html[lang|="el"] .got-tile{font-family:Arial, Sans-serif;font-weight:700;}


.gdhp-tile.got-tile.tile-sprite.js-gdhpclick(data-url!="[@T[link:<relative path='/offers/online-business.aspx'><param name='ci' value='93295' /></relative>]@T]",data-ci="93295")
  .content-wrapper
    #addition-wrapper
      .addition +
      .addition +
    #products
     .product
       .text [@L[cds.sales/homepage/tile/got:got-tile-domain]@L]
     .product
       .text [@L[cds.sales/homepage/tile/got:got-tile-wsb]@L]
     .product
       .text [@L[cds.sales/homepage/tile/got:got-tile-email]@L]
    .price-text
      .price [@T[multipleproductprice:<current productidlist="464069|101|7524" period="monthly" promocode="24681357" />]@T]
      .text [@L[cds.sales/homepage/tile/got:got-per-month]@L]
    .cta
      a#gdhp-gottile-btn.btn.btn-warning(href!="javascript:void(0)",data-ci="93295") [@L[cds.sales/homepage/tile/got:got-get-it-now]@L]
      .disclaimer [@L[cds.sales/homepage/tile/got:got-rest-text-new]@L]