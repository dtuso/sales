atlantis:webstash(type="css")
  style.
    .got-tile{background-position:0 -485px;}
    .got-tile-products>ul,.got-tile-symbols>ul{list-style:none;}
    .got-tile-products>ul{margin:0 10px 0 175px;font-size:28px;font-family:'Walsheim-Black'}
    .got-tile-products>ul>li{padding:55px 10px 20px 10px;}
    .got-tile-products>ul>li>span{font-size:18px;display:inline;margin-left:20px;font-family:'Walsheim-Regular';}
    .got-tile-symbols{float:left;position:relative;font-family:'Walsheim-Black'}
    .got-tile-symbols>ul{font-size:56px;margin:110px 0 0 10px}
    .got-tile-symbols>ul>li{padding:30px 10px 10px 10px;}
    .got-tile>.atn-btn-wrap{margin-left:20px}
    .got-tile>.atn-btn-wrap>span{font-size:36px;line-height:36px;margin:10px 20px 0 0;text-transform:uppercase;text-align:center;width:225px;}
    .got-tile>.atn-btn-wrap>span>span.got-tile-price{font-size:52px;margin-bottom:5px;color:#ff8a00;}
    .got-tile>.atn-btn-wrap>span>span.got-tile-pricetext{margin:0 auto;color:#ff8a00;max-width:200px;padding:15px 0 0 0;}
    .got-tile>.atn-btn-wrap>.btn-top-img{font-size:24px;color:#fff;background-color:#f5811f;border-bottom:4px solid #b25e18;padding:14px 12px;box-sizing:border-box;height:61px;width:160px;margin:-5px 10px 0 20px;}
    .got-tile>.atn-btn-wrap>.btn-top-img:hover{background-color:#ff9b39}
    .got-restriction-text{float:right;text-align:center;margin-top:5px;position:relative;left:-40px;font-size:12px;font-family:'Walsheim-Medium'}
    .bigtext{visibility:hidden;}
    span.got-tile-price{font-family:'Walsheim-Bold'}
    html[lang|="de"] .got-tile-products>ul{font-size:22px;}
    html[lang|="de"] .got-tile-products>ul>li{padding: 55px 10px 25px 10px;}
    html[lang|="de"] .got-tile>.atn-btn-wrap>span{width:150px;margin-top:20px;}
    html[lang|="de"] .got-tile>.atn-btn-wrap>.btn-top-img{font-size:16px;margin:10px 10px 0 30px;padding:18px 8px;}
    html[lang|="es"] .got-tile-products>ul{font-size:24px;}
    html[lang|="es"] .got-tile>.atn-btn-wrap>span{width:150px;margin:0 20px 0 25px}
    html[lang|="es-US"] .got-tile>.atn-btn-wrap>span{margin:0 20px 0 0}
    html[lang|="es"] .got-tile>.atn-btn-wrap>.btn-top-img{font-size:16px;margin:-10px 10px 0 60px;padding:18px 8px;}
    html[lang|="es"] .got-restriction-text{margin-top:0px;}
    html[lang|="fr"] .got-tile-products>ul{font-size:24px;}
    html[lang|="fr"] .got-tile-products>ul>li{padding: 45px 10px 25px 10px;}
    html[lang|="fr"] .got-tile>.atn-btn-wrap>span{width:150px;margin-left:30px}
    html[lang|="fr"] .got-tile>.atn-btn-wrap>.btn-top-img{font-size:16px;margin:10px 10px 0 45px;padding:18px 8px;}
    html[lang|="it"] .got-tile-products>ul{font-size:24px;}
    html[lang|="it"] .got-tile-products>ul>li{padding: 50px 10px 20px 10px;}
    html[lang|="it"] .got-tile>.atn-btn-wrap>span{width:150px;margin-left:30px}
    html[lang|="it"] .got-tile>.atn-btn-wrap>.btn-top-img{font-size:16px;margin:10px 10px 0 45px;padding:18px 8px;width:initial;}
    html[lang|="nb"] .got-tile>.atn-btn-wrap>span>span.got-tile-price{margin-bottom: 10px;}
    html[lang|="pt-BR"] .got-tile>.atn-btn-wrap>span{width:165px;}
    html[lang|="pt-BR"] .got-tile>.atn-btn-wrap>.btn-top-img{font-size:16px;margin:10px 10px 0 65px;padding:18px 8px;}
    html[lang|="uk"] .got-tile-products>ul,html[lang|="pt"] .got-tile-products>ul,html[lang|="ru"] .got-tile-products>ul{font-size:21px;}
    html[lang|="uk"] .got-tile-products>ul>li:nth-child(2),html[lang|="uk"] .got-tile-products>ul>li:nth-child(3),html[lang|="ru"] .got-tile-products>ul>li:nth-child(2){width:200px;}
    html[lang|="uk"] .got-tile-products>ul>li:nth-child(3){padding-top:35px;}
    html[lang|="uk"] .got-tile>.atn-btn-wrap>.btn-top-img,html[lang|="tr"] .got-tile>.atn-btn-wrap>.btn-top-img{padding-top:20px;font-size:16px;}
    html[lang|="uk"] .got-restriction-text{left:-20px;}
    html[lang|="uk"] .got-tile>.atn-btn-wrap>span>span.got-tile-pricetext,html[lang|="tr"] .got-tile>.atn-btn-wrap>span>span.got-tile-pricetext,html[lang|="el"] .got-tile>.atn-btn-wrap>span>span.got-tile-pricetext,html[lang|="ru"] .got-tile>.atn-btn-wrap>span>span.got-tile-pricetext{font-size:36px !important;}
    html[lang|="tr"] .got-tile-products>ul>li:nth-child(2){line-height:30px;padding-top:45px;padding-bottom:5px;}
    html[lang|="da"] .got-tile-products>ul,html[lang|="el"] .got-tile-products>ul{font-size:25px;}
    html[lang|="da"] .got-tile-products>ul>li{padding-top:60px;}
    html[lang|="el"] .got-tile-products>ul>li:nth-child(2){padding-top:45px;padding-bottom:5px;}
    html[lang|="el"] .got-tile-products>ul>li:nth-child(3){padding-bottom:30px;}
    html[lang|="fi"] .got-tile>.atn-btn-wrap>.btn-top-img{font-size:22px;}
    html[lang|="pt"] .got-tile>.atn-btn-wrap>.btn-top-img{font-size:19px;}

