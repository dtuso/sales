---
location: sales/homepage/marquee/gybo/default_template
id: false
name: GCE-30764
ctaLink: "[@T[link:<relative path='/offers/online-business.aspx'><param name='ci' value='93372' /></relative>]@T]"
elementId: gybo
---
extends ../../../layouts/homepage/marquee
block append marqueeStyles
  style.
    #gybo.marquee{background: #ffffff}
    .gybo-marquee{width:100%;min-width:1000px;height:400px;cursor:pointer;background-color:#ffffff}
    .gybo-marquee-content{width:1000px;margin:0 auto;}
    .gybo-text{color:#333333;padding:30px 0 0 0;}
    .gybo-text>span{display:block;}
    .gybo-text>div{display:inline-block;vertical-align:top;}
    .gybo-middle-wrapper{padding:20px 0 0 0;}
    .gybo-middle-wrapper>span{display:block;}
    .gybo-price-text{text-align:center;}
    .gybo-headline{font-size:66px;line-height:66px;}
    .gybo-product-text{font-size:28px;line-height:28px;text-align:center;}
    .gybo-product-subtext{font-family:'Walsheim-Medium';font-size:18px;line-height:18px;text-align:center;padding:5px 0 0 0;}
    .gybo-cond-text{font-size:41px;line-height:41px;display:block;margin-top:50px;}
    .gybo-rest-text{font-family:'Walsheim-Medium';font-size:12px;line-height:12px;display:block;padding-top:10px;text-align:center;}
    .gybo-btn-wrapper>.gybo-price-text{color:#ffffff;background-color:#ff8a00;width:140px;height:125px;display:block;margin:20px 0 10px 0;padding:35px 10px 0 10px;}
    .gybo-btn-wrapper>.gybo-price-text>.gybo-price{position:absolute;line-height:1.1;}
    .gybo-plus-img,.gybo-equal-img{font-size:100px;padding:20px 20px 0 20px;}
    .btn-wrap>.btn{width:130px;font-size:24px;}
    .gybo-domain-image{background:url([@T[link:<imageroot />]@T]fos/hp/rebrand/img/gybo_marquee.jpg) no-repeat 0px 0px;width:160px;height:160px;margin-bottom:10px;}
    .gybo-wsb-image{background:url([@T[link:<imageroot />]@T]fos/hp/rebrand/img/gybo_marquee.jpg) no-repeat 0px -162px;width:160px;height:160px;margin-bottom:10px;}
    .gybo-o365-image{background:url([@T[link:<imageroot />]@T]fos/hp/rebrand/img/gybo_marquee.jpg) no-repeat 0px -324px;width:160px;height:160px;margin-bottom:10px;}
    #gdhp-marq-gybo .btn {font-family:'Walsheim-Medium';}
    #gdhp-marq-gybo {font-family:'Walsheim-Regular';}
    .bigtext{visibility:hidden;}
    
    html[lang|="de"] .gybo-headline{font-size:48px;}
    html[lang|="de"] .gybo-product-text{font-size:24px;}
    html[lang|="de"] .gybo-marquee .btn-wrap>.btn{font-size:18px;}
    html[lang|="de"] .gybo-rest-text{width:168px;}

    html[lang|="it"] .gybo-headline{font-size:48px;}
    html[lang|="it"] .gybo-product-text{font-size:24px;}
    html[lang|="it"] .gybo-marquee .btn-wrap>.btn{font-size:14px;}
    html[lang|="it"] .gybo-rest-text{width:168px;}
    
    html[lang|="fr"] .gybo-headline{font-size:48px;}
    html[lang|="fr"] .gybo-product-text{font-size:24px;}
    html[lang|="fr"] .gybo-marquee .btn-wrap>.btn{font-size:14px;}
    html[lang|="fr"] .gybo-rest-text{width:168px;}

block marqueeContent
  #gdhp-marq-gybo.gybo-marquee.js-gdhpclick(data-url!="[@T[link:<relative path=\"/offers/online-business.aspx\"><param name=\"ci\" value=\"93372\" /></relative>]@T]", data-ci="93372")
    .gybo-marquee-content
      .gybo-text
        span.gybo-headline.walsheim-black [@L[cds.sales/homepage/marquee/gybo:gybo-headline]@L]
        .gybo-middle-wrapper
          span.gybo-domain-image
          span.gybo-product-text.tungsten[@L[cds.sales/homepage/marquee/gybo:gybo-marq-domain]@L]
        .gybo-plus-img.walsheim-black +
        .gybo-middle-wrapper
          span.gybo-wsb-image
          span.gybo-product-text.tungsten [@L[cds.sales/homepage/marquee/gybo:gybo-marq-wsb]@L]
        .gybo-plus-img.walsheim-black +
        .gybo-middle-wrapper
          span.gybo-o365-image
          span.gybo-product-text.tungsten [@L[cds.sales/homepage/marquee/gybo:gybo-marq-email]@L]
          span.gybo-product-subtext [@L[cds.sales/homepage/marquee/gybo:gybo-marq-o365]@L]
        .gybo-equal-img.walsheim-black =
        .gybo-btn-wrapper
          span.gybo-price-text.bigtext
            span.gybo-price.walsheim-black [@T[productprice:<current productid="464069" period="monthly" promocode="24681357"></current>]@T] 
            span.gybo-cond-text.tungsten [@L[cds.sales/homepage/marquee/gybo:gybo-per-month]@L]
          .btn-wrap
            span.btn.btn-warning#gdhp-marq-gybo-btn(data-ci="93372") [@L[cds.sales/homepage/marquee/gybo:gybo-get-it-now]@L]
            span.gybo-rest-text [@L[cds.sales/homepage/marquee/gybo:gybo-rest-text]@L]