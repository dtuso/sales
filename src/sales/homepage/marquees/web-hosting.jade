---
location: sales/homepage/marquee/web-hosting/default_template
id: false
ctaLink: "[@T[link:<relative path='/hosting/web-hosting.aspx'><param name='ci' value='97797' /></relative>]@T]"
elementId: webhost-m
---

extends ../../../layouts/homepage/marqueeLeftHeadline
block marqueeLeftHeadlineStyles
	style
		:less
			#webhost-m {font-family:'Walsheim-Black', 'Arial Black', sans-serif;
				.left-column{padding-top:100px;
					.logo{width: 250px;
						margin-bottom: 20px;
						}
					.title{font-size:36px;line-height:36px;width: 75%;}
					.subtitle{font-size:26px;color:#333333;}
			  }
				.right-column{line-height: 1.0; padding-top: 130px; margin-left:120px;
					.title{font-size:36px;line-height:36px;color:#333333;text-align: left;}
					.price{color: #ff8a00;
						.space {word-spacing:-7px}}
					button{float:left;font-family:'Walsheim-Bold', 'Arial Black', sans-serif;width:65%;
					}
				 }
			}
			.marquee{
				width: 100%;
				min-width: 1000px;
				height: 400px;
				cursor: pointer;
				color: #fff;
				background: url("data:image/png;base64,")no-repeat top center;
			}
			html[lang="el-GR"]{
				font-size: 32px;
				line-height: 32px;
			}
block leftColumn
		img.logo(src='data:image/png;base64,')
		.title [@L[cds.sales/homepage/marquee/web-hosting:product]@L]
		| 
		| ##if(productHasSavingsMoreThan(32051, 32051, 4))
		.subtitle [@L[cds.sales/homepage/marquee/web-hosting:actAndSave]@L]
		| 
		| ##endif
block rightColumn
		.title [@L[cds.sales/homepage/marquee/web-hosting:onlyProduct]@L]
		button.btn.btn-warning.btn-purchase.btn-lg.btn-cta(href!=ctaLink) [@L[cds.sales/homepage/marquee/web-hosting:cta-text]@L]