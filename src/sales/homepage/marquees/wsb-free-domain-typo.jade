		.gdhp-wsb-text{vertical-align:18px;}
		.domains-container { line-height: 1; display: inline-block; float: left; font-size: 28px; background: url([@T[link:<imageroot />]@T]fos/hp/rebrand/img/marquee-wsb-icons.png) scroll no-repeat; background-position: 0 0; width: 140px;padding-top: 160px;text-align: center;margin-right:30px;}
