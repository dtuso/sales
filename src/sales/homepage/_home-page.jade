          $("#hero-wrapper").css("min-height", $("#marquee-tile-wrapper").height());
        }
    }

    var ignoreLivePerson = true;
    ux.eldorado.ready(function(){
      var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&'),
