mixin tldBugs(tlds)
  span.tld-links  
    each tld in tlds
      a.tld-link(class=tldData[tld].cssClass href!="[@T[link:<relative path='" + tldData[tld].path + "'><param name='ci' value='" + tldData[tld].ci + "' /><param name='tld' value='" + tldData[tld].tld + "' /></relative>]@T]")
        img.tld-image(class=tldData[tld].cssClass src=tldData[tld].uri)
        span.price!= "[@T[productprice:<current productid='" + tldData[tld].productId + "' dropdecimal='false' period='yearly' htmlsymbol='false' negative='parentheses' promocode='444888'/>]@T]"
        if tldData[tld].showIcannDisclaimer
          span.icann-disclaimer *
  atlantis:webstash(type="css")
    style.
      .tld-links a { font-size: 14px; color: #333; margin-right: 30px; display: inline-block; }
      .tld-links span, .tld-links img { display: inline-block; }
      .tld-links a:hover>.price{text-decoration:underline}
      .tld-image {vertical-align: top; margin-right: 8px;}

