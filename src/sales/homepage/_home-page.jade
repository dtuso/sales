block pageStyles
  style
    :less
      @light-gray:#f5f5f5;
      @med-gray:#f3f3f4;
      @dark-gray:#dedede;
      @shadow:rgba(0,0,0,0.1);
      @dark-green:#008a30;
      @gd-orange:#ff8a00;
      h2{font-size:4rem}
      .text-white{color:#fff}
      .walsheim-regular{font-family:'Walsheim-Regular', Arial, sans-serif;}
      .walsheim-medium{font-family:'Walsheim-Medium', Arial, sans-serif;}
      .walsheim-bold{font-family:'Walsheim-Bold', 'Arial Black', sans-serif;}
      .walsheim-black{font-family:'Walsheim-Black', 'Arial Black', sans-serif;}
      .tungsten{font-family:Tungsten, 'Tungsten A', 'Tungsten B', Impact, sans-serif;}
      .search-template .search-template-box{margin-top:20px}
      .search-template .search-template-box{margin-bottom:0}
      #marquee{position:relative}
      .hero-wrapper{min-height:300px;}
      #marquee .tile-wrapper{margin-top:6.5px}
      #marquee .tile{min-height:140px;}
      #marquee .tile-wrapper:last-child{margin-bottom:6.5px}
      .tile{cursor:pointer}
      .top-banner-lower-content{min-height:40px;}
      .gdhp-share{position:relative;float:right;}
      #bugbar{margin:10px 0;}
      #domain-search{margin-top: 10px;}
      .price,.icann-disclaimer{color:@gd-orange}
      #merchandising{background-color:#fff;padding:10px 0;font-size:16px;
        .tile-wrapper{min-height:140px;margin:5px 0}
      }
      #puppy-promo{background-image:url([@T[link:<imageroot />]@T]fos/hp/rebrand/img/buddy-video.jpg)}
      .domain-promo-content{background-image:url('[@T[link:<imageroot />]@T]fos/hp/rebrand/img/domain_background.jpg');background-size:cover;min-height:420px}
      .domain-promo-text{padding-top:80px;}
      #domain-promo .text-center{color:#333;margin:0;padding:30px 0;text-transform:uppercase;}
      #domain-promo h1{display:inline;color:#333;text-transform:none;font-size:1.6rem;font-family:Arial, "Helvetica Neue", Helvetica, sans-serif;}
      #domain-promo h3{font-size:3.4rem}
      #domain-promo .btn{margin-top:20px}
      #why-godaddy{padding-bottom:70px;}
      #why-godaddy p{margin-right:15px;}
      #why-godaddy h2{color:#333}
      #why-godaddy .why-gd-prices{padding-top:40px}
      #why-godaddy .why-gd-support{padding-top:34px}
      #o365-promo{padding-bottom:105px}
      .o365-promo-text ul{list-style-image:url(data:image/png;base64,);margin:0 0 20px -12px}
      .o365-promo-image{padding-top:150px;width:100%}
      #website-promo .shape-text{color:#333}
      #website-promo ul{margin-left:-20px;}
      #website-promo li{width:125%}
      .new-customers-gd-pro{padding-bottom:50px}
      .new-customers-gd-pro img{width:42%}
      h2.two-up-title{margin:20px 0}
      .two-up-speech-shape.two-up-speech-shape-gold,.two-up-speech-shape.two-up-speech-shape-gold:after {background-color: #fedc45;}
      .two-up-speech-shape.two-up-speech-shape-gold:before {border-top-color: #fedc45;}
      #support-promo{background-color:#333;padding-bottom:50px;}
      .support-promo-img{width:60%;}
      .support-promo-number{font-size:2.0rem;margin-top:2%}
      div.video-marquee{background-color:transparent}

