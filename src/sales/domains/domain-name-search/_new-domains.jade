.container
  atlantis:webstash(type="css")
    style.
      ##if(activeLanguageAny([el-gr]))
      #new-domains ul li span, #new-domains .col-tld-title, #new-domains .bg-warning{font-weight:700;}
      ##endif
      #new-domains {padding:255px 0 70px 0;background-color: #e8e8e8;}
      #new-domains .container{margin:0 auto;padding-left:0px;}
      #new-domains ul{list-style-type:none; -webkit-padding-start:10px;}
      #new-domains ul li {background-color:#ffffff; width: 100%; height:46px;}
      #new-domains ul li span{font-family: "Walsheim-Black"; font-size: 30px; padding-left:10px;}
      #new-domains ul li a{float:right;}
      #new-domains .col-tld-title {padding-left: 10px;}
      #new-domains .col-tld-title, #new-domains .bg-warning{font-family:'Walsheim-Black';font-size:18px;}
  .row
    .col-md-6
      span.col-tld-title [@L[cds.sales/gd/domains/domain-name-search:WhyGoDaddyListTitle0Rebrand]@L] 
      span.bg-warning [@L[cds.sales/gd/domains/domain-name-search:WhyGoDaddyListTitle01]@L]
      ul
        li
          +tldMini("[@L[cds.sales/gd/domains/domain-name-search:TldDotGreen]@L]","[@L[cds.sales/gd/domains/domain-name-search:RegisterRebrand]@L]","[@T[link:<relative path='/tlds/green'><param name='ci' value='97717' /></relative>]@T]")
        li
          +tldMini("[@L[cds.sales/gd/domains/domain-name-search:TldDotWorld]@L]","[@L[cds.sales/gd/domains/domain-name-search:RegisterRebrand]@L]","[@T[link:<relative path='/tlds/world'><param name='ci' value='95891' /></relative>]@T]")
        li
          +tldMini("[@L[cds.sales/gd/domains/domain-name-search:TldDotLegal]@L]","[@L[cds.sales/gd/domains/domain-name-search:RegisterRebrand]@L]","[@T[link:<relative path='/tlds/legal'><param name='ci' value='97718' /></relative>]@T]")
        li
          +tldMini("[@L[cds.sales/gd/domains/domain-name-search:TldDotSite]@L]","[@L[cds.sales/gd/domains/domain-name-search:RegisterRebrand]@L]","[@T[link:<relative path='/tlds/site'><param name='ci' value='97719' /></relative>]@T]")  
    .col-md-6
      span.col-tld-title [@L[cds.sales/gd/domains/domain-name-search:WhyGoDaddyListTitle0Rebrand]@L] 
      span.bg-warning [@L[cds.sales/gd/domains/domain-name-search:WhyGoDaddyListTitle11]@L]
      ul
        li
          +tldMini("[@L[cds.sales/gd/domains/domain-name-search:TldDotApp]@L]","[@L[cds.sales/gd/domains/domain-name-search:LearnMore]@L]","[@T[link:<relative path='/tlds/gtld.aspx'><param name='ci' value='90059' /></relative>]@T]")
        li
          +tldMini("[@L[cds.sales/gd/domains/domain-name-search:TldDotStore]@L]","[@L[cds.sales/gd/domains/domain-name-search:LearnMore]@L]","[@T[link:<relative path='/tlds/gtld.aspx'><param name='ci' value='97716' /></relative>]@T]")
        li
          +tldMini("[@L[cds.sales/gd/domains/domain-name-search:TldDotNews]@L]","[@L[cds.sales/gd/domains/domain-name-search:LearnMore]@L]","[@T[link:<relative path='/tlds/gltd.aspx'><param name='ci' value='97715' /></relative>]@T]")
        li
          +tldMini("[@L[cds.sales/gd/domains/domain-name-search:TldDotLive]@L]","[@L[cds.sales/gd/domains/domain-name-search:LearnMore]@L]","[@T[link:<relative path='/tlds/gtld.aspx'><param name='ci' value='97714' /></relative>]@T]")