atlantis:webstash(type="css")
  style.
    #domain-not-available-marquee-view { display: none; }
    #domain-not-available-marquee-view .pro-plans .pro-plan-wrap {border-top: 10px solid #008a32; padding: 20px;}
    #domain-not-available-marquee-view .pro-plans {margin-top: 0px; }
    #domain-not-available-marquee-view .domain-not-available-heading-text  { color: #333333; }
    #domain-not-available-marquee-view .other-domains-heading-text {padding-top 20px; }
    #domain-not-available-marquee-view .results-list-heading-text {padding-top 20px; }
    #domain-not-available-marquee-view .domain-spin-wrap {min-height: 120px;border: solid 1px #cccccc;margin-bottom: 15px;} 
    #domain-not-available-marquee-view .domain-name-display {text-transform: lowercase; margin-bottom: 0px; margin-top: 0px;}
    #domain-not-available-marquee-view .domain-name-display-tld {text-transform: lowercase;margin-bottom: 0px; margin-top: 0px;}
    #domain-not-available-marquee-view .show-more-arrow { position: relative; top: 16px; left: 15px; bottom: 0; margin-left: -10px; width: 0; height: 0; border: 11px solid transparent; border-top-color: #000; content: ''; }
    #spin-results .spin-results-message,
    #spin-results .spin-template {display:none;font-size:18px;}
    #spin-results .select-and-continue {margin-bottom: 0px;}
    #spin-results .spin-results-message {margin-top:15px;}
    #spin-results .checking-availability, #spin-results .now-unavailable {padding-top:5px;}
    #domain-not-available-marquee-view


section#domain-not-available-marquee-view
  .bg-green-official
    .container
      .row
        .col-md-12
          h2.domain-not-available-heading-text [@L[cds.sales/offers/online-business:32573-sorry-domain-unavailable]@L]
      +domainSearchOffersInput('[@L[cds.sales/offers/online-business:32573-search-try-again]@L]', '12347', '[@L[cds.sales/offers/online-business:32573-try-a-different-domain-name-placeholder]@L]', '[@L[cds.sales/offers/online-business:32573-search]@L]', '[@L[cds.sales/offers/online-business:32573-type-your-business-placeholder]@L]', '[@L[cds.sales/offers/online-business:32573-eligibility-error]@L]', '[@L[cds.sales/offers/online-business:32573-offer-only-valid]@L]')
  .bg-white
    .container.pro-plans
      .row
        h4.other-domains-heading-text [@L[cds.sales/offers/online-business:32573-here-are-some-others]@L]
        h6.results-list-heading-text [@L[cds.sales/offers/online-business:32573-number-of-number-results]@L] <a>[@L[cds.sales/offers/online-business:32573-view-all-results]@L]</a>
      #spin-results
      #spin-template-wrap
        .row.spin-template.spin-result
          .col-sm-12.plan-tile.plan-pro
            .pro-plan-wrap.domain-spin-wrap
              .row
                .col-md-8.col-sm-12
                  h4.domain-name-display thisreallycoolexample
                  h2.domain-name-display-tld .com
                .col-md-4.col-sm-12.text-right
                  button.btn.btn-primary.select-and-continue.spin-results-message [@L[cds.sales/offers/online-business:32573-select-and-continue-button]@L]
                  .spin-results-message.checking-availability [@L[cds.sales/offers/online-business:32573-checking-availability]@L]
                  .spin-results-message.now-unavailable [@L[cds.sales/offers/online-business:32573-domain-no-longer-available]@L]
                  
      h6.text-center [@L[cds.sales/offers/online-business:32573-see-more-results]@L]
        span.show-more-arrow

  .sf-dialog.api-B-failure
    p [@L[cds.sales/offers/online-business:32573-generic-domain-search-error]@L]
