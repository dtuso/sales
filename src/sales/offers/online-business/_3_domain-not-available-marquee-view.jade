
style.
  #domain-not-available-marquee-view { display: none; }
  #domain-not-available-marquee-view .pro-plans .pro-plan-wrap {border-top: 10px solid #008a32; padding: 20px;}
  #domain-not-available-marquee-view .pro-plans {margin-top: 0px; }
  #domain-not-available-marquee-view .domain-not-available-heading-text  { color: #333333; }
  #domain-not-available-marquee-view .domain-not-available-subheading-text { color: #000000;}
  #domain-not-available-marquee-view .other-domains-heading-text {padding-top 20px; }
  #domain-not-available-marquee-view .results-list-heading-text {padding-top 20px; }
  #domain-not-available-marquee-view .domain-list {min-height: 120px;border: solid 1px #cccccc;margin-bottom: 15px;} 
  #domain-not-available-marquee-view .domain-name-display {text-transform: lowercase; margin-bottom: 0px; margin-top: 0px;}
  #domain-not-available-marquee-view .domain-name-display-tld {text-transform: lowercase;margin-bottom: 0px; margin-top: 0px;}
  #domain-not-available-marquee-view .show-more-arrow { position: relative; top: 16px; left: 15px; bottom: 0; margin-left: -10px; width: 0; height: 0; border: 11px solid transparent; border-top-color: #000; content: ''; }
section#domain-not-available-marquee-view
  .bg-green-official
    .container
      .row
        .col-md-12
          h2.domain-not-available-heading-text Sorry, that domain's already taken.
          h4.domain-not-available-subheading-text The right one's out there. Just try again.
      +domainSearchOffersInput('', '12347', 'Try a different domain name')
  .bg-white
    .container.pro-plans
      h4.other-domains-heading-text Or, here are some other domains you might like too&hellip;
      h6.results-list-heading-text 1 - 3 of {0} results <u>View all</u>
      .col-sm-12.plan-tile.plan-pro
        .pro-plan-wrap.domain-list
          .row
            .col-md-8
              h4.domain-name-display thisreallycoolexample
              h2.domain-name-display-tld .com
            .col-md-4.text-right
              button.btn.btn-primary Select & Continue
        .pro-plan-wrap.domain-list
          .row
            .col-md-8
              h4.domain-name-display anothercoolexample
              h2.domain-name-display-tld .co
            .col-md-4.text-right
              button.btn.btn-primary Select & Continue
        .pro-plan-wrap.domain-list
          .row
            .col-md-8
              h4.domain-name-display yetanothercoolexample
              h2.domain-name-display-tld .net
            .col-md-4.text-right
              button.btn.btn-primary Select & Continue
      h6.text-center SEE MORE RESULTS
        span.show-more-arrow
