atlantis:webstash(type="css")
  style.
    #domain-selection-view h4.other-domains-heading-text {padding-top 20px; font-size: 24px; color:#333; font-weight: bold; }
    #domain-selection-view h6.results-list-heading-text {padding-top 20px; font-size: 18px; color:#333; }
    #domain-selection-view .domain-spin-wrap {min-height: 120px;border: solid 1px #cccccc;margin-bottom: 15px;} 
    #domain-selection-view .clickable-show-more {cursor: pointer;}
    #domain-selection-view .show-more-arrow { position: relative; top: 12px; margin-left: 5px; width: 0; height: 0; border: 11px solid transparent; border-top-color: #000; content: ''; }
    #domain-selection-view button.view-all-button {font-size: 18px; color: #6586C4; font-family: Arial;}
    #domain-selection-view .not-available-domain-name-row {
      margin: 35px 0 25px;
    }

    #recommended-domain .spin-results-message,
    #spin-results .spin-results-message, 
    #spin-results .spin-result, 
    #spin-template-wrap .spin-template {display:none;}
    #domain-selection-view .select-and-continue {margin-bottom: 0px; font-size:20px;text-overflow: ellipsis;}
    #recommended-domain .spin-results-message,
    #spin-results .spin-results-message {margin-top:15px;}

    .container .row .topSpacing{padding-top:20px;}
    .container .row .leftSpacing{padding-left:50px;}
    .col-xss-1{width: 13%;float: left;position: relative;min-height: 1px;padding-right: 10px;padding-left: 10px;}
    .icon-spacing{text-align: center;font-size: 2em;}

    h2{margin-top:0px;margin-bottom:0px;}
    h5{margin-top:0px;margin-bottom:0px;}

    .container .row .features-email{height:117px; background: url([@T[link:<imageroot />]@T]fos/sales/themes/scotty/p4p/img/img-features-email.png) no-repeat center bottom;}
    .domain-icon{height:92px;background: url([@T[link:<imageroot />]@T]fos/sales/themes/scotty/p4p/img/img-domain-icon.png) no-repeat center bottom;}
    .half-hero-left{height:214px;background: url([@T[link:<imageroot />]@T]fos/sales/themes/scotty/p4p/img/img-halfGuy-left.png) no-repeat center bottom;}
    .speach-bubble-left-div {margin-left:5%;width:90%;}
    .speach-bubble-left-white {color: #000;font-family: Tungsten, 'Tungsten A', 'Tungsten B', 'Helvetica Neue', 'Segoe UI', Segoe, Helvetica, Arial, 'Lucida Grande', sans-serif;font-size: 28px;font-size: 2.8rem;padding: 10px 20px;text-transform: uppercase;line-height: 1;background: url(data:image/svg+xml;base64,) no-repeat center center;background-size: cover;overflow: visible;}

    #domain-selection-view .select-and-continue{margin:12px 0;float:right;}
    #domain-selection-view .domain-name-display{margin:15px 0;color:#333;font-size:40px;font-weight:400;display:block;line-height:1}
    // .domain-nameDiv{line-height:1}
    // .domainSelect{margin-top:10px;}

    #domain-selection-view .available-domain-name-row {
      margin: 5px 0 10px;
    }
    
    #domain-available-view h2.available-domain-name-text {
      margin: 0;
    }
recommended-domain-name
    
section#domain-selection-view.js-domain-search-wizard-section

  .bg-green-official
    .container
      .row
        div.domain-icon(style={'margin-top':'35px'})
      .row
        div.text-center(style={'margin-top':'15px'})
          h2.uppercase(style={'margin-bottom':'0px'}) domain name
      .row
        div.text-center
          h4.domain-starts.strong(style={'margin-top':'10px'})
      #recommended-domain
        .row
          div.container.white
            div.col-sm-9
              span.domain-name-display.lowercase.recommended-domain-name#recommended-domain-name
            div.col-sm-3
              button.btn.btn-primary.select-and-continue.recommended-domain-name.uppercase Select
              .spin-results-message.checking-availability [@L[cds.sales/offers/online-business:32573-checking-availability]@L]
              .spin-results-message.now-unavailable [@L[cds.sales/offers/online-business:32573-domain-no-longer-available]@L]

  .bg-white
    .container.other-domains
      .row
        // div.container
        h5(style={'margin-top':'20px'}) <strong>That one not moving you? Here are some alternatives:</strong>
      .row
        h6.results-list-heading-text 
        span#spin-counts(data-result-count-template!="[@L[cds.sales/offers/online-business:32573-number-of-number-results]@L]")
        button.btn.btn-link.view-all-button(data-ci="95269") [@L[cds.sales/offers/online-business:32573-view-all-results]@L]
      #spin-results
      #spin-template-wrap
        .row.spin-template.spin-result
          div.container.white.available-domain-name-row
            div.col-sm-9
              span.domain-name-display.lowercase
            div.col-sm-3
              button.btn.btn-primary.select-and-continue.uppercase Select
              .spin-results-message.checking-availability [@L[cds.sales/offers/online-business:32573-checking-availability]@L]
              .spin-results-message.now-unavailable [@L[cds.sales/offers/online-business:32573-domain-no-longer-available]@L]

      #show-more-section
        h6.text-center
          span.clickable-show-more(data-ci="95268") [@L[cds.sales/offers/online-business:32573-see-more-results]@L]
            span.show-more-arrow
