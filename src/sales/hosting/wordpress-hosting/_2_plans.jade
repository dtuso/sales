atlantis:webstash(type="css")
  style.
    #plans {
      margin:0 auto; 
    }
    #plans .pro-plans .pro-plan-wrap {
      padding: 35px 25px;
    }
    #plans .pro-plans .pro-plan-wrap.pro-plan-wrap-alternate {
        border-top: 10px solid #FE8D01;
    }
    #plans .plan-price-and-duration {
      margin-top: -5px;
    }
    #plans .pro-plans .pro-plan-wrap .plan-price-wrap {
      margin: 30px 0;
    }
    /*
    html[lang="en-NZ"] #plans .plan-price {
      font-size: 4.0rem;
    }
    html[lang="en-NZ"] #plans .plan-duration {
      font-size: 1.7rem;
    }
    */
    .include-check-black, .include-check-green, .include-check-none {
      padding-left: 33px;
      position: relative;
      margin-top: 20px;
    }
    .include-check-black:before {
      content: "";
      background-image: url([@T[link:<imageroot />]@T]fos/hp/sahara-rebrand-sprite-20141114.png);
      background-position: 0 -700px;
      background-size: 205px auto;
      width: 25px;
      height: 27px;
      padding-right: 5px;
      position: absolute;
      left: 0;
      top: -6px;
    }
    .include-check-green:before {
      content: "";
      background-image: url([@T[link:<imageroot />]@T]fos/hp/sahara-rebrand-sprite-20141114.png);
      background-position: 0 -668px;
      background-size: 205px auto;
      width: 25px;
      height: 27px;
      padding-right: 5px;
      position: absolute;
      left: 0;
      top: -6px;
    }
    #plans .all-plans-include h3 {
      margin: 50px 0 30px;
    }


    @media (max-width: 999px) {
      #plans .plan-tile {
        margin-top: 0px;
      }
    }
    @media (min-width: 1000px) {
    }
    .pro-banner .btn {
      word-wrap:normal;
    }


section#plans
  -includeBoxStyles = true;
  | 
  | ##if(splitTestingSideIsActive(1458, B))
  | 
  +renderCartLogic(page.config.url, page.itc, {dropdown: "true"})
  +renderPlanTiles(4) 
    +planTile("planTileA")
      include ./_2_plan-A_dropdown.jade
    +planTile("planTileB")
      include ./_2_plan-B_dropdown.jade
    +planTile("planTileC")
      include ./_2_plan-C_dropdown.jade
    +planTile("planTileD")
      include ./_2_plan-D_dropdown.jade
  | 
  | ##else
  | 
  +renderCartLogic(page.config.url, page.itc)
  -hasIncludedPlanTilesMixinJs = false;
  +renderPlanTiles(4) 
    +planTile("planTileA")
      include ./_2_plan-A.jade
    +planTile("planTileB")
      include ./_2_plan-B.jade
    +planTile("planTileC")
      include ./_2_plan-C.jade
    +planTile("planTileD")
      include ./_2_plan-D.jade
  | 
  | ##endif
  | 
  .container.all-plans-include
    +proBanner({"ci":"93999","link":"[@T[link:<relative path='/pro/managed-wordpress-hosting'><param name='ci' value='93999' /></relative>]@T]"})
    .row
      h3.text-center [@L[cds.sales/gd/hosting/wordpress-hosting:35561-all-plans-title]@L]
    .row
      .col-md-4.col-sm-12
        ul.green-check
          li [@L[cds.sales/gd/hosting/wordpress-hosting:39394_all_plans_include_1]@L]
          li
            | [@L[cds.sales/gd/hosting/wordpress-hosting:39394_all_plans_include_2]@L]
            +toolTip("[@L[cds.sales/gd/hosting/wordpress-hosting:35561-tooltip-free-domain]@L]")
          li [@L[cds.sales/gd/hosting/wordpress-hosting:39394_all_plans_include_3]@L]
          | 
          | ##if((productIsOffered(99) && !countrySiteAny(mx)) && splitTestingSideIsActive(1458, A))
          | 
          li
            | [@L[cds.sales/gd/hosting/wordpress-hosting:39394_all_plans_include_4]@L]
            +toolTip("[@L[cds.sales/gd/hosting/wordpress-hosting:35561-tooltip-office-365]@L]")  
          | 
          | ##endif
          | 
          
      .col-md-4.col-sm-12
        ul.green-check
          li [@L[cds.sales/gd/hosting/wordpress-hosting:39394_all_plans_include_5]@L]
          li [@L[cds.sales/gd/hosting/wordpress-hosting:39394_all_plans_include_6]@L]
          li [@L[cds.sales/gd/hosting/wordpress-hosting:39394_all_plans_include_7]@L]
          li
            | [@L[cds.sales/gd/hosting/wordpress-hosting:39394_all_plans_include_8]@L] 
            +toolTip("[@L[cds.sales/gd/hosting/wordpress-hosting:39394_all_plans_include_8_tooltip]@L]")


      .col-md-4.col-sm-12
        ul.green-check
          li [@L[cds.sales/gd/hosting/wordpress-hosting:39394_all_plans_include_9]@L]
            +toolTip("[@L[cds.sales/gd/hosting/wordpress-hosting:39394_all_plans_include_9_tooltip]@L]")
          li [@L[cds.sales/gd/hosting/wordpress-hosting:39394_all_plans_include_10]@L]
          li [@L[cds.sales/gd/hosting/wordpress-hosting:39394_all_plans_include_11]@L]
            +toolTip("[@L[cds.sales/gd/hosting/wordpress-hosting:35561-tooltip-guarantee]@L]")
          
