  +renderTabbedCarousel({
      backgroundColor: "",
      icode: "",
      featuresCarouselId: "carousel-security-tabs", /* the ID of the tabs area */
      title: "[@L[cds.sales/gd/hosting/wordpress-hosting:35561-security-title]@L]",
      subtitle: "[@L[cds.sales/gd/hosting/wordpress-hosting:35561-security-subtitle]@L]",
      numOfTabs: 5,
      id: "carousel-security" /* the ID of the actual carousel */
    })
    +addTab({text:"[@L[cds.sales/gd/hosting/wordpress-hosting:35561-security-slide-1-title]@L]",ci:page.ci.securityCarouselSlideTab1})
    +addTab({text:"[@L[cds.sales/gd/hosting/wordpress-hosting:35561-security-slide-2-title]@L]",ci:page.ci.securityCarouselSlideTab2})
    +addTab({text:"[@L[cds.sales/gd/hosting/wordpress-hosting:35561-security-slide-3-title]@L]",ci:page.ci.securityCarouselSlideTab3})
    +addTab({text:"[@L[cds.sales/gd/hosting/wordpress-hosting:35561-security-slide-4-title]@L]",ci:page.ci.securityCarouselSlideTab4})
