section#security(data-mid-nav-title="Security",data-cicode="#{page.ci.midPageNavSecuriy}")

  - securityCarousel = securityCarousel || {};
  - securityCarousel.lazyLoad = true;
  - securityCarousel.css = true;
  - securityCarousel.backgroundColor ='';
  - securityCarousel.ci = {};
  - securityCarousel.ci.prev = page.ci.securityCarouselSlideLeft;
  - securityCarousel.ci.next = page.ci.securityCarouselSlideRight;
  - securityCarousel.ci.carouselIndicator = page.ci.securityCarouselSlideSel;
  - securityCarouselItem = securityCarouselItem || {};
  - securityCarouselItem.textCenter = true;
  - securityCarouselItem.ci = {};
  - securityCarouselItem.ci.panelContainer = '';
  .container
    .row
      .col-sm-10.col-sm-push-1.center-block
        h2.h0.main-heading.text-center [@L[cds.sales/gd/hosting/wordpress-hosting:35561-security-title]@L]
