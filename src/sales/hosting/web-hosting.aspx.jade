extends ../../templates/pages/landing-page

prepend endOfPageScripts
  script.
    ##if(activeLanguageAny([en-us]))
      var loadReviews = function(){
        if (typeof $BV != "undefined") {
          $BV.configure("global", {
            allowSamePageSubmission: true,
            userToken: "[@P[userControl:<Data location="~/Shared/BazaarVoice/SessionUserToken.ascx"></Data>]@P]",
            doLogin: function(successCallback, successUrl) {
              $('#login-modal').modal();
            },
            doScrollSubmission: function () {
              return false;
            }
          });
          $BV.ui("rr", "show_reviews", {
            productId: "web_hosting",
            num: 1,
            doShowContent: function() {
              $('.bv-loading').hide();
            }
          });
        }
      };
      $(window).load(function() {
        if (!document.getElementById('bvapi')) {
          var bv = document.createElement('script');
          bv.type = 'text/javascript';
          bv.id = 'bvapi';
          bv.async = true;
          bv.src = '[@P[userControl:<Data location="~/Scripts/BVScriptUrlOnly.ascx"></Data>]@P]';
          bv.onload = loadReviews;
          (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(bv);
        }
      });
    ##endif
    function comparePlans(id){
      if(id=="cpanel-tab"){
        $("#parallels-tab").hide();
        $("#"+id).show();
       }else if(id =="parallels-tab"){
        $("#cpanel-tab").hide();
        $("#"+id).show();
       }
    }
    $('.dropdown-toggle').dropdown()

block pageStyles
  style
    include ./web-hosting/styles.css

block domainSearch
  //- remove domain search from top of hosting page

block marquee
  section#marquee 
    include ./web-hosting/_marquee.jade

block plans
  section#plans
    +renderPlanTiles(3) 
      +planTile
        include ./web-hosting/plans/_plan-hosting-economy.jade
      +planTile
        include ./web-hosting/plans/_plan-hosting-deluxe.jade
      +planTile
        include ./web-hosting/plans/_plan-hosting-ultimate.jade
        
      +renderCartLogic("[@T[link:<relative path=\"/hosting-config-new.aspx\" parammode=\"common\" />]@T]", "slp_hosting_4GH" )
      .tile-section
        +renderColumnList(3)
          .row
            .col-sm-12
              h3.text-center All Plans Include
          +columnListMember
            .include-check Free domain with annual plan
            .include-check Free Email Addresses
            .include-check One-click install of 200+ Free Applications
          +columnListMember
            .include-check 1 GB storage MySQL Linux and 200 MB storage Windows SQL Databases
            .include-check Flexible, easy-to-use control panel
            .include-check 24/7 security monitoring and DDoS protection
          +columnListMember
            .include-check Compatible with all major browsers
            .include-check One certificate covers unlimited servers
            .include-check One-click installation for GoDaddy hosted accounts
  
